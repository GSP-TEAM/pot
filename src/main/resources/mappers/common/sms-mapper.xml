<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.sicc.gsp.svm.gms.common.utils.dispatch.CmmSmsDAO">

	<insert id="dispatch" parameterType="com.gmsutil.dispatch.DispatchVO">
	<![CDATA[
  	INSERT
  	  INTO SMSTABLE
  	       (SENDER,
  	       RECEIVER,
  	       MSG,
  	       CRT_ID,
	       CRT_IP,
	       CRT_DATE) 
    VALUES (#{sender_mobile},
   		   #{receiver_mobile},
   		   #{msg},
	       #{username},
	       #{user_ip},
	       GETDATE()) 
	]]>
	</insert>
</mapper>